<% if user_signed_in? %>
  <%= form_with url: search_category_path, method: :get, class:"form-inline" do |f| %>
    <div class="form-group mr-2">
      <%= f.text_field :keyword, class:"form-control"%>
    </div>
    <div class="form-group">
      <%= f.submit "タグ検索", class:"btn btn-secondary"%>
    </div>
  <% end %>
<% end %>